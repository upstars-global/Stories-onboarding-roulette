/* =============================================================================
   STORIES COMPONENT STYLES
   ============================================================================= */

@import '@scss';

.story-container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  transform-origin: center center;
}

.story_controls {
  position: absolute;
  z-index: 9998;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  border: none;
  padding: unset;
  @include media(M) {
    width: auto;
    left: 50%;
    transform: translate(-50%);
  }
}

.text_container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  background: linear-gradient(
    0deg,
    #fd7f00 0%,
    #f97002 8.17%,
    #db430c 24.62%,
    #d03304 46.39%,
    #d43403 53.37%,
    #c33105 67.14%,
    #9e1a04 82.01%,
    #920f04 93.04%,
    #790303 100%
  );

  @include media(M) {
    width: 58.2dvh;
    left: 50%;
    transform: translate(-50%);
    height: 94%;
    top: 3%;
    display: flex;
    overflow: hidden;
    border-radius: 3dvh;
    filter: drop-shadow(0 0px 8px rgba(153, 75, 255, 0.6));
  }
}

#stories-segment-1 {
  z-index: 1;
}
#stories-segment-2 {
  z-index: 2;
}

#stories-segment-3 {
  z-index: 3;
}

#stories-segment-4 {
  z-index: 4;
}

#stories-segment-5 {
  z-index: 5;
}

.stories-segment {
  z-index: 1;
  pointer-events: none;
  user-select: none;
  -moz-user-select: none;
  top: to-percent-y(88px);
  left: to-percent-x(16px);
  width: to-percent-x(328px);
  height: 87%;
  height: to-percent-y(564px);
  position: absolute;
  @include media(M) {
    top: 16%;
  }
}

.h1 {
  margin-bottom: to-vh(10px);
  font-size: to-vh(28px);
  color: var(--color-text-alt);
  text-align: left;
  font-family: $font-family-primary;
  font-style: normal;
  font-weight: $font-weight-extrabold;
  position: relative;
  line-height: $line-height-snug;
  // max-width: 90%;
  @media (max-aspect-ratio: 1/2) {
    font-size: 3.5dvh;
  }
}

.h2 {
  font-size: to-vh(14px);
  color: var(--color-text-body);
  text-align: left;
  font-family: $font-family-primary;
  font-style: normal;
  font-weight: $font-weight-normal;
  position: relative;
  line-height: $line-height-relaxed;
  max-width: 100%;
  margin-top: to-vh(10px);
  word-break: break-word;
  @media (max-aspect-ratio: 1/2) {
    font-size: 1.8dvh;
  }
}

.video-player {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  height: 100%;
  width: auto;
  overflow: hidden;
  top: 0;
  user-select: none;
  pointer-events: none;
  z-index: 0;
  @include media(M) {
    height: 90%;
    top: 5%;
    border-radius: 1rem;
    filter: drop-shadow(0px 0px 15px rgba(208, 0, 255, 0.4));
  }
}

.video-container {
  height: 100%;
  z-index: 0;
  overflow: hidden;
  user-select: none;
  pointer-events: none;
}

.close_button {
  position: fixed;
  left: 0;
  margin-left: 87%;
  transform: unset;
  top: 5dvh;
  z-index: 9999;
  cursor: pointer;
  @include media(M) {
    left: 50%;
    transform: translate(23dvh);
    margin-left: 0;
    top: 11dvh;
  }
}

.pause_button {
  position: absolute;
  z-index: 500;
  top: 0;
  left: 45dvh;
  @include media(M) {
    left: 50%;
    transform: translate(18.5dvh);
    top: 11.1dvh;
  }
}

.scale_down_animation {
  animation: scale_down 5s ease-out forwards;
}

@keyframes scale_down {
  100% {
    transform: scale(0.8);
    opacity: 0.9;
  }
}

.hide {
  display: none;
}

.player_status {
  position: relative;
  height: 30dvh;
  width: 30dvh;
  aspect-ratio: 1/1;
  margin-bottom: 5dvh;
  left: 50%;
  transform: translate(-50%);
}

