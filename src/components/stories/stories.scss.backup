    94deg,
    #a432b5 5.66%,
    #d75fff 11.66%,
    #a841b7 28.49%,
    #8b29d9 51.5%,
    #7b35ed 68.33%,
    #7140ff 83.6%,
    #4b86de 93.72%,
    #4c6bda 96.6%
