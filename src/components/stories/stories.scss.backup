  border-radius: 2.3dvh;
  margin-bottom: 3dvh;
  margin-top: 3dvh;
}

.game_thumbnail {
  position: relative;
  height: 39.3dvh;
  width: 28dvh;
  aspect-ratio: 5/7;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
